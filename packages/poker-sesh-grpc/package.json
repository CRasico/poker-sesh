{
  "name": "poker-sesh-grpc",
  "version": "1.0.0",
  "description": "PokerSesh.io subset of gRPC services and clients",
  "author": "Cameron Rasico <rasicoca@gmail.com>",
  "homepage": "https://github.com/CRasico/poker-sesh#readme",
  "license": "ISC",
  "main": "./dist/index.js",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "files": [
    "src",
	"dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CRasico/poker-sesh.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build-protobuf": "sh ../../scripts/build-protos.sh -s './src/protocol-buffers' -d './dist/protocol-buffers'",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts,.tsx .",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.+(js|ts|json|tsx)\""
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "prettier": "^2.7.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.7.0",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.3.2"
  }, 
  "bugs": {
    "url": "https://github.com/CRasico/poker-sesh/issues"
  }
